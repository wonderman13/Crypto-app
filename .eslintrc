{
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "extends": "react-app",
    "rules": {
      "react/jsx-no-target-blank": 0,
      "func-names": 0,
      "max-len": 0,
      "no-unused-vars": 0,
      "no-script-url": 0,
      "react/no-unescaped-entities": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "no-console": 0,
      "no-undef": 0,
      "react/no-array-index-key": 0,
      "import/prefer-default-export": 0,
      "global-require": 0,
      "react/prop-types": 0,
      "comma-dangle": [
        "off"
      ],
      "import/extensions": 0
    },
    "plugins": [
      "react"
    ]
  }